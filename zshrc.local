DISABLE_AUTO_UPDATE=true
HOMEBREW_NO_AUTO_UPDATE=true
export ANDROID_SDK_ROOT="$HOME/development/Android/sdk/"
export PATH="$PATH:$HOME/development/flutter/bin:$HOMEBREW_HOME/opt/openjdk@11/bin"
export PATH="$HOMEBREW_HOME/opt/php@7.4/bin:$HOMEBREW_HOME/opt/php@7.4/sbin:$PATH"
export PATH="$HOMEBREW_HOME/opt/mariadb@10.3/bin:$PATH"
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion
